{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "jsx": "react",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "es5"
  },
  "extends": "expo/tsconfig.base"
}
